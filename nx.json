{
  "extends": "nx/presets/npm.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "generate-sources",
          "build",
          "dev",
          "preview",
          "deploy"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^generate-sources", "^build"]
    },
    "docker": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build"]
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
