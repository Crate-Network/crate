{
  "name": "root",
  "author": "Christian van der Loo",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jscutlery/semver": "^2.25.2",
    "@nrwl/eslint-plugin-nx": "14.3.6",
    "@nrwl/jest": "14.3.6",
    "@nrwl/js": "^14.3.5",
    "@nrwl/linter": "14.3.6",
    "@nrwl/node": "14.3.6",
    "@nrwl/react": "^14.3.5",
    "@nx-tools/docker-metadata": "^3.0.0",
    "@nx-tools/nx-docker": "^3.0.0",
    "@nxext/vite": "^14.0.0",
    "@openapitools/openapi-generator-cli": "2.5.1",
    "@trumbitta/nx-plugin-openapi": "^1.11.0",
    "@types/jest": "28.1.3",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "~5.29.0",
    "@typescript-eslint/parser": "~5.29.0",
    "eslint": "~8.18.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "^8.0.0",
    "jest": "28.1.1",
    "nx": "14.3.6",
    "ts-jest": "28.0.5",
    "ts-node": "~10.8.0"
  },
  "workspaces": [
    "packages/*",
    "clients/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:web": "nx build web",
    "build:since": "nx affected --target=build",
    "test:all": "nx run-many --target=test --all",
    "test:web": "nx test web",
    "test:since": "nx affected --target=test",
    "dev": "nx run-many --target=dev --all",
    "dev:web": "nx dev web",
    "docker": "nx run-many --target=docker --all",
    "docs": "nx run-many --target=generate-sources --all && nx run @crate/api-lib:build",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "tslib": "^2.3.0"
  },
  "version": "0.0.3"
}
