{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "node_modules", "dist", "**/*.d.ts"],
  "overrides": [
    {
      "env": {
        "es2021": true,
        "node": true
      },
      "files": ["*.ts", "*.js"],
      "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
      }
    }
  ]
}
